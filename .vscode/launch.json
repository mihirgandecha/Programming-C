{
    "configurations": [
        
        {
            "name": "(gdb) Turtle",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Turtle/parse_s", 
            "args": ["TTLs/forward.ttl"], 
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "preLaunchTask": "make run",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}